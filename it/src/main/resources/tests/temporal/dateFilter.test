{
    "name": "filter on date part",

    "backends": {
        "marklogic_json": "pending",
        "mimir": "pending"
    },

    "data": "../days.data",

    "query": "select distinct `day` from `../days` where date_part(\"dow\", ts) >= 3",

    "predicate": "exactly",
    "ignoreResultOrder": true,

    "expected": ["Wednesday", "Thursday", "Friday", "Saturday"]
}
